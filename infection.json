{
    "timeout": 10,
    "source": {
        "directories": [
            "app"
        ],
        "excludes": [
            "app/Console/Commands",
            "app/Http/Controllers",
            "app/Models"
        ]
    },
    "mutators": {
        "@default": true
    },
    "testFramework": "phpunit",
    "phpUnit": {
        "configDir": ".",
        "customPath": "vendor/bin/phpunit"
    },
    "minMsi": 80,
    "minCoveredMsi": 80,
    "ignoreMsiWithNoMutations": true,
    "initialTestsPhpOptions": "-d zend.enable_gc=0"
}
